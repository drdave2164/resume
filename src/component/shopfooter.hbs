<div class="row">
    {{#each footer}}
    <div class="nav col-2">
        <div class="nav-item">
            {{#each this}}
            <div>
                {{#if @last}}
                <a href="{{link}}" class="nav-link disabled">{{text}}</a>
                {{else}}
                <a href="{{link}}" class="nav-link">{{text}}</a>
                {{/if}}
            </div>
            {{/each}}
        </div>
    </div>
    {{/each}}
</div>